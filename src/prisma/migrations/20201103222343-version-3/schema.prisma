datasource db {
    provider = "sqlite"
    url = "***"
}

generator client {
    provider = "prisma-client-js"
}

model Data {
    id Int @id @default(autoincrement())
    loggedAt DateTime @default(now())
    data Float

}
model User {
    id  Int @id @default(autoincrement())
    name String
    email String @unique
    password String

}